{"ast":null,"code":"import React from \"react\";\nvar __jsx = React.createElement;\nimport TableHead from '@material-ui/core/TableHead';\nimport TableSortLabel from '@material-ui/core/TableSortLabel';\nimport TableRow from '@material-ui/core/TableRow';\nimport Checkbox from '@material-ui/core/Checkbox';\nimport { SortTypes } from './types';\nimport GenericTableCell from './GenericTableCell';\n\nvar EnhancedTableHead = function EnhancedTableHead(_ref) {\n  var columns = _ref.columns,\n      onSelectAllClick = _ref.onSelectAllClick,\n      order = _ref.order,\n      orderBy = _ref.orderBy,\n      numSelected = _ref.numSelected,\n      rowCount = _ref.rowCount,\n      onRequestSort = _ref.onRequestSort,\n      multipleSelect = _ref.multipleSelect;\n\n  function createHeader(column) {\n    var sortActive = orderBy === column.field;\n    return __jsx(GenericTableCell, {\n      key: column.field,\n      align: column.numeric ? 'right' : 'left',\n      sortDirection: sortActive ? order : false\n    }, __jsx(TableSortLabel, {\n      active: sortActive,\n      direction: sortActive ? order : SortTypes.Asc,\n      onClick: function onClick(e) {\n        onRequestSort(column.field);\n      }\n    }, column.header));\n  }\n\n  var headers = columns.map(createHeader);\n\n  function handleSelectAll(e) {\n    if (onSelectAllClick) {\n      onSelectAllClick(e.target.checked);\n    }\n  }\n\n  return __jsx(TableHead, null, __jsx(TableRow, null, __jsx(GenericTableCell, {\n    padding: \"checkbox\"\n  }, multipleSelect && __jsx(Checkbox, {\n    indeterminate: numSelected > 0 && numSelected < rowCount,\n    checked: rowCount > 0 && numSelected === rowCount,\n    onChange: handleSelectAll\n  })), headers));\n};\n\nexport default EnhancedTableHead;","map":null,"metadata":{},"sourceType":"module"}