{"ast":null,"code":"import _extends from \"@babel/runtime/helpers/esm/extends\";\nvar __jsx = React.createElement;\nimport React from 'react';\nimport ReactDOM from 'react-dom';\nimport { ThemeProvider } from '@material-ui/core/styles';\nimport BaseTheme from '../BaseTheme';\n\nfunction baseConfirm(DialogContent, dialogProps) {\n  var confirmRoot = document.querySelector('#confirmation-message-placeholder');\n  return new Promise(function (res) {\n    var giveAnswer = function giveAnswer(answer) {\n      ReactDOM.unmountComponentAtNode(confirmRoot);\n      res(answer);\n    };\n\n    ReactDOM.render(__jsx(ThemeProvider, {\n      theme: BaseTheme\n    }, __jsx(DialogContent, _extends({}, dialogProps, {\n      giveAnswer: giveAnswer\n    }))), confirmRoot);\n  });\n}\n\nexport default baseConfirm;","map":null,"metadata":{},"sourceType":"module"}